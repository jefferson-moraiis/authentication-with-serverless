{
  "name": "authentication-with-serverless",
  "version": "1.0.0",
  "description": "authentication serverless",
  "scripts": {
    "postinstall": "patch-package",
    "lint": "eslint --fix \"**/*.ts\"",
    "offline": "sls offline start --stage local --host 127.0.0.1 --useChildProcesses",
    "start": "npm-run-all -p offline build:watch",
    "start:debug": "npm-run-all  --coverage -p offline:debug build:watch",
    "build:watch": "tsc --build -w .",
    "test": "jest --coverage --collect-coverage",
    "test:debug": "jest --watchAll --silent --noStackTrace",
    "test:coverage": "jest --collect-coverage",
    "coverage": "jest --reporter=text --reporter=text-summary --reporter=lcov --report-dir=coverage --coverage",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:hml": "serverless deploy --stage hml",
    "deploy:prd": "serverless deploy --stage prd"
  },
  "author": "Jefferson Morais",
  "private": true,
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/serverless": "^3.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fkill-cli": "^7.0.0",
    "git-commit-msg-linter": "^4.0.7",
    "jest": "^27.4.7",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.5.1",
    "serverless": "2.71.0",
    "serverless-domain-manager": "^6.0.0",
    "serverless-dynamodb-local": "^1.0.2",
    "serverless-offline": "^8.4.0",
    "serverless-offline-dynamodb-streams": "^5.0.0",
    "serverless-plugin-split-stacks": "^1.11.3",
    "serverless-plugin-typescript": "^2.1.1",
    "serverless-plugin-warmup": "^5.2.2",
    "serverless-prune-plugin": "^2.0.1",
    "serverless-scriptable-plugin": "^1.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "nyc": {
    "all": true,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 85
  },
  "dependencies": {
    "@middy/core": "^2.5.7",
    "@middy/warmup": "^1.4.0",
    "async.queue": "^0.5.2",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1066.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^15.0.0",
    "dynamoose": "^2.8.5",
    "firebase": "^9.6.5",
    "firebase-admin": "^10.0.2",
    "formidable": "^2.0.1",
    "jsforce": "^1.11.0",
    "jsonwebtoken": "^8.5.1",
    "nock": "^13.2.4",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.2.2",
    "uuid": "^8.3.2"
  }
}