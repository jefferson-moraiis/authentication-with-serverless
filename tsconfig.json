{
  "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "lib": ["es2019"],
      "module": "CommonJS",
      "moduleResolution": "node",
      "target": "es2019",
      "locale": "pt-BR",
      "outDir": "build",
      "allowJs": true,
      "sourceMap": true,
      "skipLibCheck": true,
      "removeComments": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "strictNullChecks": false,
      "preserveConstEnums": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "typeRoots": [
          "node_modules/@types",
          "./types"
      ]
  },
  "exclude": [
      "build",
      ".dynamodb",
      ".nyc_output",
  ],
}